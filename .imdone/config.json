{
  "exclude": [
    "^(node_modules|bower_components|\\.imdone|target|build|dist|logs)[\\/\\\\]?|\\.(git|svn|hg|npmignore)|\\~$|\\.(jpg|png|gif|swp|ttf|otf)$"
  ],
  "meta": {
    "issue": {
      "urlTemplate": "https://bitbucket.org/janlindblom/ruby-dovado/issues/%s",
      "titleTemplate": "Bitbucket issue #%s"
    },
    "case": {
      "urlTemplate": "https://jaerto.fogbugz.com/?%s",
      "titleTemplate": "FogBugz case %s"
    }
  },
  "watcher": true,
  "keepEmptyPriority": false,
  "code": {
    "include_lists": [
      "TODO",
      "DOING",
      "DONE",
      "PLANNING",
      "FIXME",
      "ARCHIVE",
      "HACK",
      "CHANGED",
      "XXX",
      "IDEA",
      "NOTE",
      "REVIEW"
    ]
  },
  "lists": [
    {
      "name": "TODO",
      "hidden": false
    },
    {
      "name": "DOING",
      "hidden": false
    },
    {
      "name": "FIXME",
      "hidden": false
    },
    {
      "name": "DONE",
      "hidden": false
    },
    {
      "name": "copyright",
      "hidden": true
    },
    {
      "name": "PLANNING",
      "hidden": false
    }
  ],
  "marked": {
    "gfm": true,
    "tables": true,
    "breaks": false,
    "pedantic": false,
    "smartLists": true,
    "langPrefix": "language-"
  }
}
